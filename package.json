{
  "name": "matcha",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint . --ext .vue,.js,.ts --fix --ignore-path .gitignore",
    "lint:style": "stylelint \"./**/*.{vue,css,scss,sass}\" --fix --ignore-path .gitignore",
    "format": "prettier --write ./**/*.{html,vue,js,ts,jsx,tsx,json,md} --ignore-path .gitignore",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@formkit/auto-animate": "1.0.0-beta.6",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@imengyu/vue3-context-menu": "^1.2.10",
    "@msgpack/msgpack": "^2.8.0",
    "@tauri-apps/api": "^1.4.0",
    "@unocss/reset": "^0.54.0",
    "@vueuse/components": "^10.0.0",
    "@vueuse/core": "^10.0.0",
    "@vueuse/rxjs": "^10.0.0",
    "ant-design-vue": "^3.2.17",
    "clipboard": "^2.0.11",
    "dayjs": "^1.11.7",
    "dexie": "^3.2.3",
    "highlight.js": "^11.7.0",
    "js-base64": "^3.7.5",
    "linkify-string": "^4.1.1",
    "linkifyjs": "^4.1.1",
    "overlayscrollbars": "^2.1.1",
    "overlayscrollbars-vue": "^0.5.1",
    "pinia": "^2.0.35",
    "pinia-plugin-persistedstate": "^3.1.0",
    "tributejs": "^5.1.3",
    "vue": "3.2.47",
    "vue-inline-svg": "^3.1.2",
    "vue-router": "^4.1.6",
    "vue-virtual-scroller": "2.0.0-beta.8",
    "vue3-video-play": "1.3.1-beta.6",
    "waveform-path": "^0.0.1"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.16",
    "@tauri-apps/cli": "^1.4.0",
    "@types/node": "^18.15.12",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@unocss/eslint-config": "^0.52.0",
    "@unocss/preset-attributify": "^0.54.0",
    "@unocss/preset-icons": "^0.54.0",
    "@unocss/preset-uno": "^0.54.0",
    "@unocss/transformer-directives": "^0.54.0",
    "@vitejs/plugin-vue": "4.2.3",
    "@vitest/coverage-c8": "^0.31.0",
    "@vitest/ui": "^0.31.0",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.11.0",
    "happy-dom": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "postcss-html": "^1.5.0",
    "postcss-preset-env": "^8.3.2",
    "prettier": "^2.8.7",
    "stylelint": "^15.5.0",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "typescript": "^5.0.4",
    "unocss": "^0.54.0",
    "unocss-preset-scrollbar": "^0.2.1",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^4.3.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^4.3.0",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.31.0",
    "vue-eslint-parser": "^9.1.1",
    "vue-tsc": "^1.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx}": [
      "eslint --fix",
      "vitest related --run"
    ],
    "*.{vue,css,scss,sass}": "stylelint --fix",
    "*.{html,vue,js,ts,jsx,tsx,json,md}": "prettier --write --ignore-unknown"
  }
}
