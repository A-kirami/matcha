{
  "name": "matcha",
  "private": true,
  "version": "0.2.5",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint . --ext .vue,.js,.ts --fix --ignore-path .gitignore",
    "lint:style": "stylelint \"./**/*.{vue,css,scss,sass}\" --fix --ignore-path .gitignore",
    "format": "prettier --write ./**/*.{html,vue,js,ts,jsx,tsx,json,md} --ignore-path .gitignore",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.1",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@imengyu/vue3-context-menu": "^1.3.5",
    "@msgpack/msgpack": "^2.8.0",
    "@tauri-apps/api": "2.0.0-beta.3",
    "@tauri-apps/plugin-log": "2.0.0-beta.1",
    "@tauri-apps/plugin-os": "2.0.0-beta.1",
    "@tauri-apps/plugin-websocket": "2.0.0-beta.1",
    "@unocss/reset": "^0.58.0",
    "@vueuse/components": "^10.7.0",
    "@vueuse/core": "^10.7.0",
    "@vueuse/rxjs": "^10.7.0",
    "ant-design-vue": "^3.2.20",
    "clipboard": "^2.0.11",
    "dayjs": "^1.11.10",
    "dexie": "^3.2.4",
    "highlight.js": "^11.9.0",
    "js-base64": "^3.7.5",
    "linkify-string": "^4.1.3",
    "linkifyjs": "^4.1.3",
    "overlayscrollbars": "^2.4.5",
    "overlayscrollbars-vue": "^0.5.6",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "tributejs": "^5.1.3",
    "vue": "3.3.12",
    "vue-inline-svg": "^3.1.2",
    "vue-router": "^4.2.5",
    "vue-virtual-scroller": "2.0.0-beta.8",
    "vue3-video-play": "1.3.1-beta.6",
    "waveform-path": "^0.0.1"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.26",
    "@tauri-apps/cli": "2.0.0-beta.4",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@unocss/eslint-config": "^0.58.0",
    "@unocss/preset-attributify": "^0.58.0",
