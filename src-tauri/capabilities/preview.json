{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "preview",
  "description": "Capability for the preview window",
  "windows": ["preview"],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "core:window:allow-minimize",
    "core:window:allow-toggle-maximize",
    "core:window:allow-close",
    "core:window:allow-start-dragging",
    "dialog:allow-save",
    "os:allow-os-type"
  ]
}
