{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "about",
  "description": "Capability for the about window",
  "windows": ["about"],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "shell:allow-open",
    "os:allow-os-type",
    "window:allow-minimize",
    "window:allow-close",
    "window:allow-start-dragging"
  ]
}
