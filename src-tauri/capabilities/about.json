{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "about",
  "description": "Capability for the about window",
  "windows": ["about"],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "core:window:allow-minimize",
    "core:window:allow-close",
    "core:window:allow-start-dragging",
    "shell:allow-open",
    "os:allow-os-type"
  ]
}
