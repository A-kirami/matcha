{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "window:allow-minimize",
    "window:allow-toggle-maximize",
    "window:allow-close",
    "window:allow-start-dragging",
    "webview:allow-create-webview-window",
    "log:default",
    "websocket:default",
    "os:allow-arch",
    "os:allow-platform",
    "os:allow-os-type",
    "os:allow-version"
  ]
}
